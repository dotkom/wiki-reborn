{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "name": "Article",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "body": {
      "type": "richtext",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
      "type": "text"
    },
    "protection_level": {
      "type": "enumeration",
      "enum": [
        "protected",
        "limited",
        "open"
      ]
    },
    "contributors": {
      "plugin": "users-permissions",
      "collection": "user"
    },
    "history": {
      "type": "json"
    },
    "views": {
      "type": "biginteger",
      "default": "0"
    },
    "portal": {
      "model": "portal"
    }
  }
}
